<section>
<div class="container">
    <div class="row d-grid gap-2 d-md-flex justify-content-md-end mt-4">
        <a href="/admin/add-products" class="btn btn-success Add-produtcs-button-editing">Add Products</a>
    </div>
<table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Title</th>
      <th scope="col">Category</th>
      <th scope="col">Discription</th>
      <th scope="col">Image</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.category}}</td>
      <td>{{this.discription}}</td>
      <td><img class="admin-images"  src="{{this.image}}" alt=""></td>
    </tr>
    {{/each}}
  </tbody>
</table>
</div>
</section>