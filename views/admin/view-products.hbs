<section>
<div class="container">
    <div class="row d-grid gap-2 d-md-flex justify-content-md-end mt-4">
        <a href="/admin/add-products" class="btn btn-success Add-produtcs-button-editing">Add Products</a>
    </div>
<table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Title</th>
      <th scope="col">Category</th>
      <th scope="col">Price</th>
      <th scope="col">Description</th>
      <th scope="col">Image</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.Name}}</td>
      <td>{{this.Category}}</td>
      <td>{{this.Price}}</td>
      <td>{{this.Description}}</td>
      <td><img class="admin-images"  src="{{this.Url}}" alt=""></td>

    </tr>
    {{/each}}
  </tbody>
</table>
</div>
</section>