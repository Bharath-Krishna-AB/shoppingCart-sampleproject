<section>
<div class="container">
<table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">Item</th>
      <th scope="col">Title</th>
      <th scope="col">Quantity</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
        <td><img src="{{this.Url}}" alt="" style="width: 50px;height: 50px;"></td>
      <td>{{this.Name}}</td>
      <td>
          <button class="bg-secondary" style="float: left;" onclick="num()">-</button>
          <p style="float: left;" class="ps-2">1</p>
          <button class="bg-secondary ms-2" style="float: left;" onclick="num1()">+</button>
          </td>
      <td><a href="/admin/delete-products?id={{this._id}}" class="btn btn-danger" onclick="return confirm('Are you sure to delete {{this.Name}}')">Delete</a></td>
    </tr>
    {{/each}}
  </tbody>
</table>
</div>
</section>

